{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "//#format:check": {},
    "//#format:write": {},
    "//#lint": { "dependsOn": ["^build"] },
    "lint": { "dependsOn": ["^build"] },
    "//#typecheck": { "dependsOn": ["^build"] },
    "typecheck": { "dependsOn": ["^build"] },
    "build": {
      "dependsOn": ["lint", "typecheck"],
      "outputs": [".build/**"]
    }
  }
}
